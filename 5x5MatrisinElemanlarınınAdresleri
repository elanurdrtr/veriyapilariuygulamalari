//Soru:5x5'lik bir matrisin elemanlar覺n覺n adreslerini ekrana yaz覺n覺z.
#include <iostream>
#include <cstdlib>
#include <cmath>
using namespace std;

int main() {
	int dizi[5][5] = {{1,0,0},{1,0,0},{1,1,1}};
	for(int i=0;i<5;i++){
		for(int j=0;j<5;j++){
			cout<<&dizi[i][j]<<"\t";
		}
	cout<<endl;
	}
	return 0;
}
